int firstMissingPositive(int* nums, int numsSize) {
    int map[10000];
    memset(map,0,10000*sizeof(int));
    for(int i=0;i<numsSize;i++)
    { 
        if(nums[i]>0)
        {
            map[nums[i]]++;
        }
    }
    for(int j=1;j<10001;j++)
    {
        if(map[j]==0)
        {
            return j;
        }
    }
    return -1;
}
